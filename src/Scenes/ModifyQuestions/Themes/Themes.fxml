<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="500.0" prefWidth="800.0" stylesheets="@../../../../resources/css/style.css"
            xmlns="http://javafx.com/javafx/11.0.1" fx:controller="Scenes.ModifyQuestions.Themes.ThemesController">
    <children>
        <TabPane fx:id="themesInterface" layoutX="-14.0" prefHeight="419.0" prefWidth="800.0"
                 tabClosingPolicy="UNAVAILABLE">
        </TabPane>
        <Pane fx:id="panButton" layoutY="443.0" prefHeight="42.0" prefWidth="800.0">
            <children>
                <Button layoutX="150.0" layoutY="6.0" mnemonicParsing="false" onAction="#handleButtonAdd" text="Add">
                    <font>
                        <Font size="13.0"/>
                    </font>
                </Button>
                <Button layoutX="242.0" layoutY="6.0" mnemonicParsing="false" onAction="#handleButtonDelete"
                        text="Delete">
                    <font>
                        <Font size="13.0"/>
                    </font>
                </Button>
                <Button layoutX="736.0" layoutY="6.0" mnemonicParsing="false" onAction="#handleButtonBack" text="Back">
                    <font>
                        <Font size="17.0"/>
                    </font>
                </Button>
                <TextField fx:id="deleting" layoutX="312.0" layoutY="9.0" prefHeight="25.0" prefWidth="38.0"/>
                <Label layoutX="412.0" layoutY="11.0" text="Level">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <ComboBox fx:id="level" layoutX="462.0" layoutY="9.0" onAction="#changeLevel" prefHeight="25.0"
                          prefWidth="104.0"/>
            </children>
       </Pane>
   </children>
</AnchorPane>
